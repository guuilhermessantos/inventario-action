{
  "expo": {
    "name": "Invent√°rio",
    "slug": "inventario",
    "platforms": ["ios", "android"],
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.actionsys.inventario",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "package": "com.actionsys.inventario",
      "versionCode": 4
    },
    "androidStatusBar": {
      "backgroundColor": "#f3f3f3",
      "translucent": true,
      "hidden": false,
      "barStyle": "dark-content"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
